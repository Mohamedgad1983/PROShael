# Environment Configuration Example
# Copy this file to .env and update with your actual values

# Database Configuration
DATABASE_URL=your-database-url
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# CRITICAL: JWT Configuration
# This MUST be the same across all deployments
# Use a strong, unique secret in production
JWT_SECRET=alshuail-universal-jwt-secret-2024-production-32chars

# Token TTL Configuration
ADMIN_JWT_TTL=7d
MEMBER_JWT_TTL=30d

# Test Member Configuration
ALLOW_TEST_MEMBER_LOGINS=true
TEST_MEMBER_PASSWORD=123456

# Server Configuration
PORT=5001
NODE_ENV=production

# CORS Configuration
ALLOWED_ORIGINS=https://alshuail-admin.pages.dev,http://localhost:3002,http://localhost:3000
